{
  "version": 2,
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "nx build server --prod",
        "outputDirectory": "dist/apps/server",
        "nodeVersion": "18.x",
        "startCommand": "node dist/apps/server/main.js"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/server/main.js"
    }
  ]
}
