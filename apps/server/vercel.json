{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "nx run server:build:production",
        "outputDirectory": "dist/apps/server",
        "startCommand": "node dist/apps/server/main.js"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/server/main.js"
    }
  ]
}
