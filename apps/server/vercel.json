{
  "version": 2,
  "builds": [
    {
      "src": "apps/server/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "nx build server",
        "outputDirectory": "dist/apps/server"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/apps/server/$1"
    }
  ]
}
